DROP DATABASE IF EXISTS mcs105_db;
CREATE DATABASE mcs105_db;
USE mcs105_db;
CREATE TABLE discounts (
id INT NOT NULL,
title VARCHAR(255) NOT NULL
) ENGINE=Columnstore;
LOAD DATA INFILE 'DATADIR/mcs105.csv' INTO TABLE discounts FIELDS TERMINATED BY ',';;

#
# Columnstore engine doesn't support COMPRESS() function
# Author: Bharath, bharath.bokka@mariadb.com
#
-- source ../include/have_columnstore.inc

--disable_warnings
DROP DATABASE IF EXISTS mcs268_db;
--enable_warnings

CREATE DATABASE mcs268_db;
USE mcs268_db;

CREATE TABLE t1
(
 t1_INT INT,
 t1_DECIMAL DECIMAL(12,5),
 t1_TEXT TEXT,
 t1_DATE DATE,
 t1_TIME TIME
 )ENGINE=Columnstore;
INSERT INTO t1 VALUES(103, 1234.5699, 'pqrstuvwxyz', '1997-12-12', '22:12:02');
INSERT INTO t1 VALUES(-7299, 111.99, 'abcdefghijklm', '2001-1-1', '23:59:59');
INSERT INTO t1 VALUES(9913, 98765.4321, repeat('q', 5), '09-12-11', '01:08:59');

SELECT LENGTH(COMPRESS(256)) FROM t1 LIMIT 1;
SELECT LENGTH(COMPRESS('256')) FROM t1 LIMIT 1;
SELECT LENGTH(COMPRESS('a')) FROM t1 LIMIT 1;
SELECT LENGTH(COMPRESS('ab')) FROM t1 LIMIT 1;
SELECT LENGTH(COMPRESS('1')) FROM t1 LIMIT 1;
SELECT LENGTH(COMPRESS('pqrstuvw')) FROM t1 LIMIT 1;
SELECT LENGTH(COMPRESS('mariadb!@#$')) FROM t1 LIMIT 1;
SELECT LENGTH(COMPRESS('!@')) FROM t1 LIMIT 1;

--error ER_CHECK_NOT_IMPLEMENTED
SELECT t1_INT, COMPRESS(t1_INT) FROM t1 ORDER BY 1;

--error ER_CHECK_NOT_IMPLEMENTED
SELECT t1_DECIMAL, COMPRESS(t1_DECIMAL) FROM t1 ORDER BY 1;

--error ER_CHECK_NOT_IMPLEMENTED
SELECT t1_TEXT, COMPRESS(t1_TEXT) FROM t1 ORDER BY 1;

--error ER_CHECK_NOT_IMPLEMENTED
SELECT t1_DATE, COMPRESS(t1_DATE) FROM t1 ORDER BY 1;

--error ER_CHECK_NOT_IMPLEMENTED
SELECT t1_TIME, COMPRESS(t1_TIME) FROM t1 ORDER BY 1;

# Clean UP
DROP DATABASE mcs268_db;
